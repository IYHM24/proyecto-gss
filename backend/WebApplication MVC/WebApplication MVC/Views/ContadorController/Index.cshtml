@model WebApplication_MVC.Models.Contador
@{
    ViewData["Title"] = "Contador";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


    <div class="row">
          <h3>Contador</h3>
    </div>

    <div class="row mt-5">
            <table class="table table-bordered table-striped">
                <thead>
                    <tr>
                        <td>@Html.DisplayNameFor(m => m.ventas_por_dia)</td>
                        <td>@Html.DisplayNameFor(m => m.ventas_por_mes)</td>
                        @*<td>Acciones</td>*@
                    </tr>
                </thead>
                <tbody>
                        <tr>
                            <td>@Model.ventas_por_dia</td>
                            <td>@Model.ventas_por_mes</td>
                        </tr
                </tbody>
            </table>
    </div>
